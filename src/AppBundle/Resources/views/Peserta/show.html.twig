{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Peserta</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Timestamp</th>
                <td>{{ entity.timestamp|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>{{ entity.gender }}</td>
            </tr>
            <tr>
                <th>Origin</th>
                <td>{{ entity.origin }}</td>
            </tr>
            <tr>
                <th>Pod</th>
                <td>{{ entity.pod }}</td>
            </tr>
            <tr>
                <th>Bod</th>
                <td>{{ entity.bod }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ entity.phone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Present</th>
                <td>{{ entity.present }}</td>
            </tr>
            <tr>
                <th>Print</th>
                <td>{{ entity.print }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('peserta') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('peserta_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
